{
    "name": "savvy-ai/laravel-savvy-ai",
    "version": "0.3.0",
    "description": "Domain knowledge artificial intelligence framework for Laravel",
    "keywords": [
        "laravel",
        "savvy-ai",
        "artificial intelligence",
        "domain knowledge",
        "knowledge graph",
        "ai"
    ],
    "license": "MIT",
    "autoload": {
        "files": [
            "./src/helpers.php"
        ],
        "psr-4": {
            "SavvyAI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SavvyAI\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Selvin Ortiz",
            "email": "selvin@savvyai.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/broadcasting": "^10.4",
        "illuminate/console": "^10.0",
        "illuminate/contracts": "^10.0",
        "illuminate/database": "^10.4",
        "illuminate/events": "^10.4",
        "illuminate/queue": "^10.4",
        "illuminate/routing": "^10.4",
        "illuminate/support": "^10.0",
        "openai-php/client": "^0.4.0",
        "symfony/http-client": "^6.2",
        "nyholm/psr7": "^1.5",
        "vanderlee/php-sentence": "^1.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    },
    "sort-packages": true,
    "extra": {
        "laravel": {
            "providers": ["SavvyAI\\Provider"]
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.3",
        "mockery/mockery": "^1.5"
    }
}
